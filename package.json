{
  "name": "media-theme-jump-start",
  "version": "0.0.1",
  "type": "module",
  "description": "A Media Theme starter with batteries included for Media Chrome",
  "keywords": [
    "media chrome",
    "media chrome theme",
    "video",
    "player",
    "web component",
    "custom element"
  ],
  "main": "src/index.js",
  "files": [
    "src",
    "dist"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "scripts": {
    "dev": "web-dev-server",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@open-wc/dev-server-hmr": "^0.1.2-next.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/pluginutils": "^4.2.1",
    "@web/dev-server": "^0.1.30",
    "@web/dev-server-rollup": "^0.3.15",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.12.0",
    "prettier": "^2.6.1",
    "rollup": "^2.70.2"
  },
  "peerDependencies": {
    "media-chrome": "0.6.0"
  },
  "dependencies": {
    "@github/template-parts": "0.5.0"
  }
